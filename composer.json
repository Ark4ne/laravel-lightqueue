{
  "name": "akr4ne/lightqueue",
  "description": "A Manager of multithreading queue processing for laravel.",
  "keywords": [
    "laravel",
    "queue",
    "multithreading"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Allegret Guillaume"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "illuminate/console": "~4.0",
    "illuminate/support": "~4.0",
    "illuminate/queue": "~4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "satooshi/php-coveralls": "~0.6"
  },
  "autoload": {
    "psr-4": {
      "Ark4ne\\LightQueue\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
