{
  "name": "akr4ne/lightqueue",
  "description": "A Manager of multithreading queue processing for laravel.",
  "keywords": [
    "laravel",
    "queue",
    "multithreading"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Allegret Guillaume"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "illuminate/support": "4.2.*",
    "illuminate/config": "4.2.*",
    "illuminate/console": "4.2.*",
    "illuminate/queue": "4.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "orchestra/testbench": "2.2.*",
    "mockery/mockery": "*",
    "satooshi/php-coveralls": "~0.6"
  },
  "autoload": {
    "psr-4": {
      "Ark4ne\\LightQueue\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
